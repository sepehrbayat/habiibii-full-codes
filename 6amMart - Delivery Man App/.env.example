# 6amMart Flutter Delivery App - Environment Configuration

## API Configuration
BASE_URL=http://127.0.0.1:8000/api
API_TIMEOUT=30000

## Environment
ENVIRONMENT=development
DEBUG_MODE=true

## Location & Tracking
GOOGLE_MAPS_API_KEY=
LOCATION_TRACKING_ENABLED=true
REAL_TIME_LOCATION_UPDATE_INTERVAL=5000

## Firebase Configuration
FIREBASE_PROJECT_ID=
FIREBASE_API_KEY=
FIREBASE_MESSAGING_SENDER_ID=
FIREBASE_APP_ID=
FIREBASE_DATABASE_URL=

## Firebase Cloud Messaging (FCM)
FCM_SERVER_KEY=

## Payment Gateways
# Stripe for Delivery Payments
STRIPE_PUBLISHABLE_KEY=

# Razorpay for Delivery Payments
RAZORPAY_KEY_ID=

## Authentication
JWT_TOKEN=

## Delivery Features
ENABLE_DELIVERY_TRACKING=true
ENABLE_REAL_TIME_NOTIFICATIONS=true
ENABLE_AUDIO_NOTIFICATIONS=true
ENABLE_VOICE_CALLS=true

## Analytics & Monitoring
GOOGLE_ANALYTICS_ID=
ENABLE_PERFORMANCE_MONITORING=true

## Push Notifications
PUSH_NOTIFICATION_ENABLED=true

## Reporting
ENABLE_DELIVERY_REPORTS=true
ENABLE_PERFORMANCE_ANALYTICS=true

## Offline Support
ENABLE_OFFLINE_MODE=true

## Development Settings
ENABLE_LOGGING=true
ENABLE_PERFORMANCE_PROFILING=true
